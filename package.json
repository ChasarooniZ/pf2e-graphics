{
	"name": "pf2e-graphics",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "npm run build:vite && npm run build:clean",
		"build:vite": "vite build",
		"build:sonda": "cross-env SONDA=true npm run build:vite",
		"build:clean": "tsx ./scripts/cleanJSONs.ts",
		"build:schema": "tsx ./scripts/buildJSONSchema.ts",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint . --cache",
		"lint:fix": "eslint . --fix --cache",
		"changelog": "changelog --format markdownlint --create --release",
		"test": "npm run test:assets && npm run test:animations && npm run lint:fix",
		"test:animations": "tsx ./scripts/tests/animations.ts",
		"test:assets": "npm run test:assets:exist && npm run test:assets:format",
		"test:assets:exist": "tsx ./scripts/tests/assets/exist.ts",
		"test:assets:format": "tsx ./scripts/tests/assets/format.ts",
		"generate-spritesheet": "tsx ./scripts/generateSpritesheetJSON.ts"
	},
	"imports": {
		"#runtime/*": "@typhonjs-fvtt/runtime/*",
		"#standard/*": "@typhonjs-fvtt/standard/*"
	},
	"dependencies": {
		"@typhonjs-fvtt/runtime": "^0.2.0-next.9",
		"@typhonjs-fvtt/standard": "^0.2.0-next.6"
	},
	"devDependencies": {
		"@actions/core": "^1.11.1",
		"@antfu/eslint-config": "^4.2.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tsconfig/svelte": "^5.0.4",
		"@typed-foundry/socketlib": "^1.0.12",
		"@types/jquery": "^3.5.32",
		"@types/lodash": "^4.17.15",
		"autoprefixer": "^10.4.18",
		"cross-env": "^7.0.3",
		"eslint": "^9.20.1",
		"eslint-plugin-antfu": "^3.0.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-svelte": "^3.0.0-next.18",
		"foundry-pf2e": "github:7H3LaughingMan/foundry-pf2e",
		"jb2a-databases": "github:Jules-Bens-Aa/jb2a-databases",
		"json-source-map": "github:MrVauxs/json-source-map",
		"keep-a-changelog": "^2.5.3",
		"lodash": "^4.17.21",
		"picocolors": "^1.1.1",
		"postcss": "^8.5.1",
		"postcss-minify": "^1.1.0",
		"postcss-prefixwrap": "^1.54.0",
		"sequencer": "github:fantasycalendar/FoundryVTT-Sequencer",
		"sonda": "^0.7.1",
		"svelte": "^4.2.19",
		"svelte-check": "^4.1.4",
		"svelte-eslint-parser": "^0.43.0",
		"svelte-jsoneditor": "^1.1.1",
		"svelte-multiselect": "^11.0.0-rc.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.8.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"vite": "^6.1.0",
		"vite-plugin-checker": "^0.8.0",
		"vite-tsconfig-paths": "^5.1.3",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.1",
		"zod-validation-error": "^3.4.0"
	}
}
